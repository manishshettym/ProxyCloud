-")>-1?optUrl=_getDraftOptUrl(optUrl,games):options&&options.scoresCollection===!0?(optUrl+="?_ceID="+options.pubKey,optUrl+="es"===lang?"&contentorigin=deportes":"&contentorigin=espn"):topic.indexOf("-topevents")>-1&&sb.settings.topEventsId?optUrl+="?_ceID="+sb.settings.topEventsId:sportAndLeague.sport&&sportAndLeague.league&&(optUrl=optUrl+"?sport="+sportAndLeague.sport+"&league="+sportAndLeague.league),optUrl&&(optUrl+=(optUrl.indexOf("?")===-1?"?":"&")+"lang="+lang+"&region="+region,i18n.espnPlusEnabled===!0&&(optUrl+="&showAirings=true")),espn.flow.connect(fastCastConnectionGroup.topic,handleCasterMessage,{url:optUrl,error:_handleUpdates})},timeToConnect))}function _unsubscribeFromFastCastTopic(topic,subscriber){var fastCastConnectionGroup=fastCastConnectionGroups[topic],subscriberIndex=0;return fastCastConnectionGroup&&(subscriberIndex=fastCastConnectionGroup.subscribers.indexOf(subscriber))!==-1?(fastCastConnectionGroup.subscribers.splice(subscriberIndex,1),fastCastConnectionGroup.subscribers.length>0?fastCastConnectionGroup:(null!==fastCastConnectionGroup.connectTimeout&&(clearTimeout(fastCastConnectionGroup.connectTimeout),fastCastConnectionGroup.connectTimeout=null),fastCastConnectionGroup.connected?(sbDebug&&window.console.log("disconnect from",fastCastConnectionGroup.topic),espn.flow.disconnect(fastCastConnectionGroup.topic,handleCasterMessage)):sbDebug&&window.console.log("stop waiting to connect to",fastCastConnectionGroup.topic),delete fastCastConnectionGroups[topic],fastCastConnectionGroup)):fastCastConnectionGroup}function unsubscribeFromAllFastCastTopics(subscriber){Object.keys(fastCastConnectionGroups).forEach(function(topic){_unsubscribeFromFastCastTopic(topic,subscriber)})}function _changeTopics(view){var newTopic,activeLeague,currentTopic=sb.topics.scoreboard;"topEvents"===sb.activeLeague?newTopic=sb.helpers.getTopicForTopEvents():(activeLeague=sb.leagues.findWhere({active:!0}),activeLeague&&(newTopic=sb.helpers.getTopicForLeague(activeLeague))